	<select id="get" resultType="JobStationDataOrigin">
		SELECT 
			<include refid="jobStationDataOriginColumns"/>
		FROM job_station_data_origin a
		<include refid="jobStationDataOriginJoins"/>
		WHERE a.id = #{id}
	</select>
	
	<select id="findList" resultType="JobStationDataOrigin">
		SELECT 
			<include refid="jobStationDataOriginColumns"/>
		FROM job_station_data_origin a
		<include refid="jobStationDataOriginJoins"/>
		<where>
			
			<if test="stationCode != null and stationCode != ''">
				AND a.station_code = #{stationCode}
			</if>
			<if test="queryBegintime != null and queryBegintime != ''">
				AND a.time &gt;= #{queryBegintime}
			</if>
			<if test="queryEndtime != null and queryEndtime != ''">
			AND a.time &lt;= #{queryEndtime}
			</if>
		</where>
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
			</otherwise>
		</choose>
	</select>
	
