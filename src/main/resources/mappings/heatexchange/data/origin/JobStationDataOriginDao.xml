	<select id="getStationNewestData" resultType="JobStationDataOrigin">
		SELECT
		<include refid="jobStationDataOriginColumns"/>
		FROM job_station_data_origin a
		<include refid="jobStationDataOriginJoins"/>
		<where>
			AND a.station_code = #{stationCode}
		</where>
		ORDER BY TIME DESC
		LIMIT 1
	</select>

